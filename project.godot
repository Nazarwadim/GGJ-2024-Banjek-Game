config_version=5

[application]

config/name="Ggj-2024-banjek-game"
run/main_scene="res://resources/scenes/school/school.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.623529, 0.564706, 0.580392, 1)
config/icon="res://icon.png"

[autoload]

BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
GuiTransitions="*res://addons/simple-gui-transitions/singleton.gd"
SignalBus="*res://resources/global_scripts/signal_bus.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/initial_position_type=0
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/anim_player_refactor/plugin.cfg", "res://addons/finite_state_machine/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/simple-gui-transitions/plugin.cfg", "res://addons/utility_ai/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "green",
"res://resources/": "yellow"
}

[filesystem]

import/fbx/enabled=false

[gui_transitions]

config/default/auto_start=true
config/default/fade_layout=true
config/default/center_pivot=true
config/default/duration=0.5
config/default/delay=0.5
config/default/transition_type="QUAD"
config/default/ease_type="IN_OUT"
config/default/animation_enter=4
config/default/animation_leave=4

